<root xmlns:xlink="http://www.w3.org/1999/xlink">
  <g id="acyl-filter">
    <filter id="filter" x="-50%" y="-50%" width="200%" height="200%">
      <feImage id="feImage1" xlink:href="#support1" result="result1"/>
      <feComposite id="feComposite1" in="result1" in2="SourceAlpha" operator="out"/>
    </filter>
    <use id="support1" style="stroke-opacity:1;stroke:url(#acyl-gradient);stroke-width:2.0" xlink:href="#acyl-drawing"/>
  </g>
  <g id="acyl-visual">
    <use id="visible1" transform="translate(24,24) scale(1.00) translate(-24,-24)" style="fill:url(#acyl-gradient);filter:url(#filter)" xlink:href="#acyl-drawing"/>
  </g>
</root>
