<root xmlns:xlink="http://www.w3.org/1999/xlink">
  <g id="acyl-filter">
    <filter id="filter">
      <feColorMatrix id="feColorMatrix1" result="fbSourceGraphicAlpha" in="SourceGraphic" values="1 0 0 -1 0 1 0 1 -1 0 1 0 0 -1 0 -0.8 -1 0 3.7 -2.4 "/>
      <feGaussianBlur id="feGaussianBlur1" stdDeviation="0.5" in="fbSourceGraphicAlpha" result="result0"/>
      <feSpecularLighting id="feSpecularLighting1" result="result1" specularConstant="0.75" specularExponent="6" in="result0" surfaceScale="7">
        <feDistantLight id="feDistantLight1" azimuth="235" elevation="15"/>
      </feSpecularLighting>
      <feDiffuseLighting id="feDiffuseLighting2" surfaceScale="20" diffuseConstant="0.2" result="result6" lighting-color="rgb(255,255,255)" in="result0">
        <feDistantLight id="feDistantLight2" azimuth="235" elevation="25"/>
      </feDiffuseLighting>
      <feBlend id="feBlend1" in2="SourceGraphic" result="result2" mode="screen"/>
      <feComposite id="feComposite1" in2="result2" operator="arithmetic" k2="1" k3="1" in="result1"/>
      <feComposite id="feComposite2" in2="SourceGraphic" operator="in" />
    </filter>
  </g>
  <g id="acyl-visual">
    <use id="visible1" style="fill:url(#acyl-gradient);filter:url(#filter)" xlink:href="#acyl-drawing"/>
  </g>
</root>

